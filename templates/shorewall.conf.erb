DROP_DEFAULT="Drop"
REJECT_DEFAULT="Reject"
ACCEPT_DEFAULT="none"
QUEUE_DEFAULT="none"
NFQUEUE_DEFAULT="none"
IP_FORWARDING=<%= @ip_forwarding %>
ADD_IP_ALIASES=No
ADD_SNAT_ALIASES=No
RETAIN_ALIASES=No
MARK_IN_FORWARD_CHAIN=No
CLAMPMSS=<%= @clampmss %>
ROUTE_FILTER=<%= @routefilter ? 'Yes' : 'No'  %>
DETECT_DNAT_IPADDRS=No
MUTEX_TIMEOUT=60
ACCOUNTING_TABLE=filter
BASIC_FILTERS=No
IGNOREUNKNOWNVARIABLES=No
AUTOCOMMENT=Yes
AUTOHELPERS=Yes
CHAIN_SCRIPTS=Yes
DEFER_DNS_RESOLUTION=Yes
EXPORTMODULES=Yes
HELPERS=
INLINE_MATCHES=Yes
IPSET_WARNINGS=Yes
LEGACY_FASTSTART=Yes
REJECT_ACTION=
RESTORE_ROUTEMARKS=Yes
SAVE_ARPTABLES=No
TRACK_RULES=No
USE_PHYSICAL_NAMES=No
USE_RT_NAMES=No
WARNOLDCAPVERSION=Yes
PROVIDER_BITS=8
PROVIDER_OFFSET=0
MASK_BITS=8
ZONE_BITS=0
MODULE_SUFFIX=ko
DISABLE_IPV6=Yes
BRIDGING=No
DYNAMIC_ZONES=No
PKTTYPE=Yes
NULL_ROUTE_RFC1918=No
